import java.util.*;
class Main{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int m = sc.nextInt();
        int[] arr1 = new int[m];
        for(int i=0;i<m;i++){
            arr1[i] = sc.nextInt();
        }
        int[] arr2 = new int[m];
        for(int i=0;i<m;i++){
            arr2[i] = arr1[m-1-i];
        }
        System.out.println(Arrays.toString(arr2));
    }
}
