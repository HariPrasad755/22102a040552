import java.util.*;
class Main{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int m = sc.nextInt();
        int[] arr1 = new int[m];
        for(int i=0;i<m;i++){
            arr1[i] = sc.nextInt();
        }
        int sum=0;
        for(int i=0;i<m;i++){
            sum = sum+arr1[i];
        }
        int avg= sum/m;
        System.out.println("Output:"+sum);
        System.out.println("Output:"+avg);
        
    }
}
        
